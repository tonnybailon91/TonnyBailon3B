//EJERCICIO 3 Leer tres nÃºmeros y escribir el mayor de los tres 

internal class Program
{
    private static void Main(string[] args)
    {
        int numero1;
        int numero2; 
        int numero3;

        Console.Write("ingrese el primer numero: ");
        numero1 = int.Parse(Console.ReadLine());
            
        Console.Write("ingrese el segundo numero: ");
        numero2 = int.Parse(Console.ReadLine());
            
        Console.Write("ingrese el tercer numero: ");
        numero3 = int.Parse(Console.ReadLine());
        
            if (numero1 >= numero2 && numero1 >= numero3)
            {
                Console.Write("el numero mayor es: " + numero1);
            }
            else if (numero2 >= numero1 && numero2 >= numero3)
            {
                Console.Write("el numero mayor es: " + numero2);
            }
            else if (numero3 >= numero1 && numero3 >= numero1)
            {
                Console.Write("el numero mayor es: " + numero3);
            }
            Console.ReadKey();
    }
}
